   "source": [
    "import pandas as pd\n",
    "\n",
    "def find_customers(customers: pd.DataFrame, orders: pd.DataFrame) -> pd.DataFrame:\n",
    "    result = customers[~customers['id'].isin(orders['customerId'].unique())][['name']]\n",
    "    result = result.rename(columns={'name':'Customers'})\n",
    "    \n",
    "    return result"
   ]
