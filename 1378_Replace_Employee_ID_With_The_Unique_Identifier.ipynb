    "import pandas as pd\n",
    "\n",
    "def replace_employee_id(employees: pd.DataFrame, employee_uni: pd.DataFrame) -> pd.DataFrame:\n",
    "    result = employees.merge(employee_uni, how='left', on='id')[['unique_id', 'name']]\n",
    "    return result"
