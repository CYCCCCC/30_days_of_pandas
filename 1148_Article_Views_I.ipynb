   "source": [
    "import pandas as pd\n",
    "\n",
    "def article_views(views: pd.DataFrame) -> pd.DataFrame:\n",
    "    result = views[views['author_id']==views['viewer_id']][['author_id']].drop_duplicates().rename(columns={'author_id':'id'})\n",
    "    result = result.sort_values('id', ascending=True)\n",
    "    return result"
   ]
  }
