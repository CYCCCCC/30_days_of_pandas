    "import pandas as pd\n",
    "\n",
    "def order_scores(scores: pd.DataFrame) -> pd.DataFrame:\n",
    "    scores['rank'] = scores['score'].rank(method='dense', ascending=False)\n",
    "    return scores[['score', 'rank']].sort_values('score', ascending=False)"
   ]
