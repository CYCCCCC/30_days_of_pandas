   "source": [
    "import pandas as pd\n",
    "\n",
    "def find_classes(courses: pd.DataFrame) -> pd.DataFrame:\n",
    "    class_cnt = courses.groupby('class')['student'].count().reset_index()\n",
    "    result = class_cnt[class_cnt['student']>=5][['class']]\n",
    "    return result"
   ]
  }
